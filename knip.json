{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"ignoreDependencies": [
		"pino-pretty",
		"pino-roll",
		"sonic-boom",
		"lint-staged"
	],
	"ignoreBinaries": ["vsce", "semgrep"],
	"ignore": [
		"nanocoder-dummy-file.ts",
		"source/utils/message-queue.tsx",
		"source/vscode/**/*.ts",
		"source/hooks/useVSCodeServer.tsx",
		"plugins/**/*",
		"scripts/fetch-models.js",
		".husky/**",
		".github/**",
		"source/components/interactive-question-prompt.tsx",
		"source/components/mode-selection-prompt.tsx",
		"source/components/plan-review-prompt.tsx",
		"source/utils/plan/slug-generator.ts",
		"source/utils/plan/phase-transition-detector.ts",
		"source/context/mode-context.ts",
		"source/utils/mode-selection-registry.ts",
		"source/utils/plan-review-registry.ts",
		"source/utils/question-selection-registry.ts",
		"source/utils/plan/plan-prompt.ts"
	],
	"ignoreExportsUsedInFile": {
		"interface": true,
		"type": true
	},
	"rules": {
		"duplicates": "off"
	},
	"tags": ["-@internal"]
}
